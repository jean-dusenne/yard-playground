{
  "name": "jean-dusenne",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --https",
    "build": "vue-tsc --noEmit && vite build --base=/yard-playground/",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "deploy": "gh-pages -d dist"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@vueuse/core": "^10.4.1",
    "file-saver": "^2.0.5",
    "floating-vue": "^2.0.0-beta.24",
    "html2canvas": "^1.4.1",
    "sanitize.css": "^13.0.0",
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@types/file-saver": "^2.0.7",
    "@types/jsdom": "21.1.6",
    "@vitejs/plugin-vue": "^2.3.4",
    "@vitest/coverage-v8": "^1.0.1",
    "@vue/eslint-config-prettier": "8.0.0",
    "@vue/eslint-config-typescript": "10.0.0",
    "@vue/test-utils": "^2.4.3",
    "@vue/tsconfig": "0.4.0",
    "c8": "^8.0.1",
    "dotenv": "^16.3.1",
    "eslint": "8.48.0",
    "eslint-plugin-vue": "8.7.1",
    "gh-pages": "^6.0.0",
    "jsdom": "^22.1.0",
    "prettier": "3.0.3",
    "sass": "^1.66.1",
    "typescript": "^5.2.2",
    "vite": "^2.9.14",
    "vitest": "^1.0.1",
    "vue-tsc": "^1.8.25"
  },
  "prettier": {
    "printWidth": 140,
    "singleQuote": true,
    "arrowParens": "avoid",
    "trailingComma": "none"
  }
}
