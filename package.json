{
  "name": "jean-dusenne",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --https",
    "build": "vue-tsc --noEmit && vite build --base=/yard-playground/",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "@vueuse/core": "^8.7.5",
    "file-saver": "^2.0.5",
    "floating-vue": "^2.0.0-beta.17",
    "html2canvas": "^1.4.1",
    "sanitize.css": "^13.0.0",
    "vue": "^3.2.37",
    "vue-hotjar-next": "^1.4.0"
  },
  "devDependencies": {
    "@types/file-saver": "^2.0.5",
    "@types/jsdom": "16.2.14",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/eslint-config-prettier": "7.0.0",
    "@vue/eslint-config-typescript": "10.0.0",
    "@vue/test-utils": "^2.0.2",
    "@vue/tsconfig": "0.1.3",
    "c8": "^7.11.3",
    "dotenv": "^16.0.1",
    "eslint": "8.19.0",
    "eslint-plugin-vue": "8.7.1",
    "gh-pages": "^4.0.0",
    "jsdom": "^20.0.0",
    "prettier": "2.7.1",
    "sass": "^1.53.0",
    "typescript": "^4.7.4",
    "vite": "^2.9.13",
    "vitest": "^0.17.0",
    "vue-tsc": "^0.38.8"
  },
  "prettier": {
    "printWidth": 140,
    "singleQuote": true,
    "arrowParens": "avoid",
    "trailingComma": "none"
  }
}
