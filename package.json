{
  "name": "jean-dusenne",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --https",
    "build": "vue-tsc --noEmit && vite build --base=/yard-playground/",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "@vueuse/core": "^10.2.0",
    "file-saver": "^2.0.5",
    "floating-vue": "^2.0.0-beta.20",
    "html2canvas": "^1.4.1",
    "sanitize.css": "^13.0.0",
    "vue": "^3.2.47",
    "vue-hotjar-next": "^1.4.0"
  },
  "devDependencies": {
    "@types/file-saver": "^2.0.5",
    "@types/jsdom": "21.1.1",
    "@vitejs/plugin-vue": "^2.3.4",
    "@vitest/coverage-v8": "^0.32.2",
    "@vue/eslint-config-prettier": "7.1.0",
    "@vue/eslint-config-typescript": "10.0.0",
    "@vue/test-utils": "^2.4.0",
    "@vue/tsconfig": "0.4.0",
    "c8": "^7.12.0",
    "dotenv": "^16.0.3",
    "eslint": "8.43.0",
    "eslint-plugin-vue": "8.7.1",
    "gh-pages": "^5.0.0",
    "jsdom": "^22.1.0",
    "prettier": "2.8.8",
    "sass": "^1.63.6",
    "typescript": "^5.1.3",
    "vite": "^2.9.14",
    "vitest": "^0.32.2",
    "vue-tsc": "^1.8.1"
  },
  "prettier": {
    "printWidth": 140,
    "singleQuote": true,
    "arrowParens": "avoid",
    "trailingComma": "none"
  }
}
